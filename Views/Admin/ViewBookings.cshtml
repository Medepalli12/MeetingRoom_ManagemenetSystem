@model IEnumerable<MeetingManagementSystem.Models.Bookings>

@{
    ViewBag.Title = "ViewBookings";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.bookingid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.userid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roomid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.starttime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.endtime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.isfoodneeded)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.totalseats)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.totalcost)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.advamount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.remainamount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.bookingid)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.userid)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.roomid)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.starttime)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.endtime)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.isfoodneeded)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.totalseats)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.status)
    </td>
    <td>
        $ @Html.DisplayFor(modelItem => item.totalcost)
    </td>
    <td>
        $ @Html.DisplayFor(modelItem => item.advamount)

    </td>
    <td>
        $ @Html.DisplayFor(modelItem => item.remainamount)
    </td>
</tr>
}

</table>
